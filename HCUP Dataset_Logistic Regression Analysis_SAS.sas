run;

proc freq data=sid.sid2013;
tables neomat;
run;

proc freq data=sid.sid2013;
tables RACE_X RACE;
run;

/*
Normal Deliver = CSEC = 0
Cesarean Section = CSEC = 1
*/

data new;
set sid.sid2013;

ARRAY DXCCS(*) DXCCS1-DXCCS25;
ARRAY PRCCS(*) PRCCS1-PRCCS14;

DO i= 1 TO 25;
	IF DXCCS(i)=196 THEN CSEC=0;
END;

DO i= 1 TO 14; 
	IF PRCCS(i)=134 then CSEC=1;
end;
run;

proc freq data=new;
tables csec;
run;

proc freq data=new;
tables csec*race/chisq;
run;

/*  figure counts women and not the # of live births  */

data new2;
set new;
if gender=0 then delete;
if age < 15  then delete;
if age >45 then delete;
if csec=. then delete;
keep ADATE AGE KEY csec AGEGROUP RACE_X RACE ECODEn ANESTH APGAR1 ASCHED ATYPE CM_OBESE FEMALE MARITALSTATUS 
MARITALSTATUSUB04 MARITALSTATUS_X
HISPANIC_X NDX NECODE PL_NCHS PRn PSTATE SEX ZIPINC_QRTL Pay1_N CM_DM CM_DMCX CM_HTN_C CM_DEPRESS DIED MEDINCSTQ; 
run; 

 /* add PAY1 for insurance payment and CM_DM is for diabetes, 
uncomplicated...CM_DMCX - AHRQ comorbidity measure: diabetes with chronic complications 
CM_HTN_C - AHRQ comorbidity measure: hypertension (combine uncomplicated and complicated) */ 

proc format;
value race 1='white' 2='black' 3='hisp' 4='asian' 5='native-am' 6='other';
value obese 0='not obese' 1='obese'; 
value csec 0='natural birth' 1='C-section';
value pay1_N 1= 'medicaremix' 2='medicaid' 3='bluecrossbcppo'4='commercialppomix' 5='privatehmo' 6='selfpay'
7='nocharge' 8='titlev' 9='workercomp' 10='CHAMPUS, CHAMPVA' 11='othergov' 12='other';
value CM_DM 0='not diabetes, uncomplicated' 1='diabetes, uncomplicated';
value CM_DMCX 0='not diabetes with chronic complications' 1='diabetes with chronic complications';
value CM_HTN_C 0='no hypertension' 1='hypertension';
value CM_DEPRESS 0='no depression' 1='depression';
value DIED 0='did not die' 1='died';
value MEDINCSTQ 1='first quartile' 2='second quartile' 3='third quartile' 4='fourth quartile';

/*create binary variable.... locate pregnancy variable and women who give birth...identify those women who were pregnant and 
gave birth without c-section*/

proc sort data=new2;
by key;
run;

