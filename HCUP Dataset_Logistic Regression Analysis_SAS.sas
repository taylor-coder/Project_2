class race (ref='white')/param=ref;
model csec (ref='0')=race;
format race race.;
run;

/* DEPRESSION ANALYSIS */

proc freq data=sid.merged;
tables (race CM_DEPRESS)*csec/chisq;
format race race. CM_DEPRESS CM_DEPRESS. csec csec.;
run;

proc logistic data=sid.merged;
class race (ref='white')/param=ref;
class CM_DEPRESS (ref='no depression')/param=ref;
model csec (ref='0')=cm_depress;
format race race. CM_DEPRESS CM_DEPRESS.;
run;

proc logistic data=sid.merged;
class race (ref='white')/param=ref;
class CM_DEPRESS (ref='no depression')/param=ref;
model csec (ref='0')=race;
format race race. CM_DEPRESS CM_DEPRESS.;
run;

proc logistic data=sid.merged;
class race (ref='white')/param=ref;
class CM_DEPRESS (ref='no depression')/param=ref;
model csec (ref='0')=race cm_depress;
format race race. CM_DEPRESS CM_DEPRESS.;
run;

