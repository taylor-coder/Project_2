data sev.sev2013;
set sev.sidsev2013;
run;

proc sort data=sev.sev2013;
by key;
run;

data sid.merged;
merge new2 sev.sev2013;
by KEY;
run;

proc freq data=sid.merged;
tables race;
format race race.;
run;

proc freq data=sid.merged;
tables (race)*csec/chisq;
format race race. csec csec.;
run;

proc freq data=sid.merged;
tables (race)*race/chisq;
format race race. cm_obese obese. csec csec.;
run;

proc freq data=sid.merged;
tables (race)*CM_HTN_C/chisq;
format race race. CM_HTN_C CM_HTN_C. csec csec.;
run;

proc freq data=sid.merged;
tables (race)*CM_DMCX/chisq;
format race race. CM_DMCX CM_DMCX. csec csec.;
run;

proc freq data=sid.merged;
tables (race)*CM_DM/chisq;
format race race. CM_DM CM_DM. csec csec.;
run;

proc freq data=sid.merged;
tables (race)*MEDINCSTQ/chisq;
format race race. MEDINCSTQ MEDINCSTQ. csec csec.;
run;

proc freq data=sid.merged;
tables (race)*obese/chisq;
format race race. obese obese. csec csec.;
run;



